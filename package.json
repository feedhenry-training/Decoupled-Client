{
  "name": "fh-decoupled",
  "version": "0.1.0",
  "author": "FeedHenry <support@feedhenry.com>",
  "description": "Decoupled builds using Phonegap SDK.",
  "dependencies": {},
  "devDependencies": {
    "grunt": "~0.4.1",
    "grunt-contrib-clean": "0.3.x",
    "grunt-contrib-copy": "0.3.x"
  },
  "ios" : {
    "packagesPhone": ["phone"],
    "packagesTablet" : [""],
    "packages" : [""],
    "dir": "ios/www/",
    "type": "universal",
    "js" : "resources/cordova/ios/*"
  },
  "android" : {
    "packagesPhone" : ["phone"],
    "packagesTablet" : [""],
    "packages" : ["phone"],
    "dir": "android/assets/www/",
    "type" : "universal",
    "js" : "resources/cordova/android/*"
  }
}

